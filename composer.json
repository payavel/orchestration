{
    "name": "payavel/serviceable",
    "description": "Service generator for Laravel.",
    "license": "MIT",
    "authors": [
        {
            "name": "Robert Kujawa",
            "email": "robert.kujawa@proton.me"
        }
    ],
    "autoload": {
        "psr-4": {
