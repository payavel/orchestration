{
    "name": "payavel/orchestration",
    "description": "Service generator for Laravel.",
    "license": "MIT",
    "authors": [
        {
            "name": "Robert Kujawa",
            "email": "robert.kujawa@proton.me"
        }
    ],
    "autoload": {
        "psr-4": {
            "Payavel\\Orchestration\\": "src/",
            "Payavel\\Orchestration\\Database\\Factories\\": "database/factories/",
            "Payavel\\Orchestration\\Tests\\Traits\\": "tests/Traits"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Payavel\\Orchestration\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "require": {
        "laravel/prompts": "^0.1.11"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Payavel\\Orchestration\\OrchestrationServiceProvider"
            ]
        }
    }
}
